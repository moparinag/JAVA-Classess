package Collections;

import java.util.ArrayList;
import java.util.List;

public class ArrayList1 {

	public static void main(String[] args) {
		List al= new ArrayList();
		System.out.println(al.size());
		al.add(45);
	al.add("Java");
	al.add("Gmail");
	al.add(678);
	al.add(45.98);
	System.out.println(al);
	System.out.println(al.size());
	for(Object o:al)
	{
		System.out.println(o);
	}
	}

}
