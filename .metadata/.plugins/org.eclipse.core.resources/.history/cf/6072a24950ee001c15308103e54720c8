package EclipsePrograms;

public class Box {
int lenght;
int breadth;
int height;
 Box()
{
	
}
 Box(int l,int b,int h)
 {
	lenght=l;
	breadth=b;
	height=h;
	
 }
 Box(int l)
 {
	 lenght=breadth=height=l;
 }
 public void Display()
 {
	 System.out.println("lenght="+lenght+", breadth="+breadth+", Height="+height);
 }
	
}
